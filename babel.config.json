{
    "presets": [
        [
            "@babel/preset-env",
            {
                "forceAllTransforms": true,
                "targets": {
                    "browsers": "> 0.25%, not dead"
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true
            }
        ]
    ]
}